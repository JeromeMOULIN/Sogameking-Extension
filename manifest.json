{
    "manifest_version": 2,
    "name": "Sogameking Twitch",
    "description": "La chaine twitch de votre streameur favoris. Ne rate plus aucun live avec la Chextention!",
    "version": "1.0.2",
    "author": "KaNa",
    "permissions": ["webNavigation"],
